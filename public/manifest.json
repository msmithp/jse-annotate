{
    "manifest_version": 3,
    "name": "jse-annotate",
    "version": "1.0.0",
    "action": {},
    "content_scripts": [
        {
            "matches": [
                "*://www.indeed.com/jobs?*",
                "*://www.indeed.com/viewjob*",
                "*://www.indeed.com/q*",
                "*://www.ziprecruiter.com/jobs-search?*"
            ],
            "js": ["background.js"],
            "css": ["style.css"]
        }
    ],
    "permissions": ["scripting", "activeTab"]
}